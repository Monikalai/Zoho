#include <stdio.h>
#include<string.h>
char stack[100];
int top=-1;
void rev(char a[],int n){
    int i,j,t;
    for(i=0,j=n-1;i<j;i++,j--){
        t=a[i];
        a[i]=a[j];
        a[j]=t;
    }
}
void push(char ch){
    stack[++top]=ch;
}
char pop(){
    int to=top;
    top--;
    return stack[to];
}
int isEmpty(){
    if(top==-1)
      return 1;
    else
      return 0;
}
int main()
{    
    char s[100];
    scanf("%s",s);
    int i;
    for(i=0;s[i];i++){
        if(s[i]!='[' && s[i]!=']')
          push(s[i]);
    }
  
    
    while(!isEmpty()&& (stack[0]>='0' && stack[0]<='9')){
        char ss[100]="",str[100]="";
        int j=0;
        while(!(stack[top]>='0' && stack[top]<='9')){
            ss[j++]=pop();
        }
        ss[j]='\0';
        int num=pop()-'0';
        while(num--){
            strcat(str,ss);
        }
        rev(str,strlen(str));
       for(i=0;str[i];i++)
          push(str[i]);
    }
    for(i=0;i<=top;i++)
    printf("%c",stack[i]);
    return 0;
}
